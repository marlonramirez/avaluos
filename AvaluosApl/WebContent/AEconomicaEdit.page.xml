<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/AEconomicaList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{aEconomicaHome.wire}"/>

   <param name="aEconomicaFrom"/>
   <param name="aEconomicaIdAEconomica" value="#{aEconomicaHome.aEconomicaIdAEconomica}"/>


   <navigation from-action="#{aEconomicaHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/AEconomica.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{aEconomicaHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/AEconomica.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{aEconomicaHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/AEconomicaList.xhtml"/>
      </rule>
   </navigation>

</page>
